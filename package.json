{
  "name": "npm-doctor",
  "version": "1.0.0",
  "description": "Search github issues based on the node modules you've installed locally",
  "main": "index.js",
  "bin": {
    "npm-doctor": "./bin/cli.js"
  },
  "scripts": {
    "test": "istanbul cover _mocha -- --recursive test",
    "lint": "eslint bin/*.js helpers/*.js lib/*.js test/**/*.js"
  },
  "keywords": [
    "npm",
    "issues",
    "github",
    "git",
    "node",
    "doctor",
    "local",
    "packages",
    "locally",
    "installed",
    "packages",
    "search",
    "find"
  ],
  "author": "szarringhalam",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.1",
    "github": "^0.2.4",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "rewire": "^2.5.1",
    "sinon": "^1.17.2"
  }
}
