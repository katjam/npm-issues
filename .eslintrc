{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "new-cap": [
            2,
            {
                "capIsNew": false

            }
        ],
        "no-underscore-dangle": [
            0
        ],
        "no-extra-bind": [
            2
        ],
        "strict": [
            2,
            "global"
        ],
        "hoist": [
            0
        ],
        "lines-around-comment": [
            2,
            {
                "beforeBlockComment": true,
                "beforeLineComment": true,
                "allowBlockStart": true
            }
        ],
        "no-lonely-if": [
            2
        ],
        "space-before-function-paren": [
            2,
            "always"
        ],
        "space-before-blocks": [
            2,
            "always"
        ],
        "space-infix-ops": [
            2,
            {
                "int32Hint": false
            }
        ],
        "space-return-throw-case": [
            2
        ],
        "spaced-comment": [
            2,
            "always"
        ],
        "no-unreachable": [
            1
        ],
        "no-debugger": [
            2
        ],
        "eqeqeq": [
            2
        ]

    },
    "env": {
        "node": true
    }
}